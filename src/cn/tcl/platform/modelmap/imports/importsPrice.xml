<?xml version="1.0" encoding="UTF-8" ?>
<import>
	<mapping bean="cn.tcl.platform.shop.vo.Shop">
		<property column="*Country" propertyName="partyId" required="true" javaType="java.lang.String"></property>
		<property column="*Branch model" propertyName="branchModel" required="true" javaType="java.lang.String"></property>
		<property column="*Price" propertyName="price" required="true" javaType="java.lang.Float"></property>
	</mapping>
	<volidate dateFormat="yyyy-MM-dd" volidateClass="cn.tcl.platform.modelmap.imports.ValidationImportModelPrice" volidateMethod="validationImport"></volidate>
	<service serviceBean="modelmapService" serviceMethod="importModelPrice"></service>
</import>